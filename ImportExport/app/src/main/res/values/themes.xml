        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ie_purple_500</item>
        <item name="colorPrimaryVariant">@color/ie_purple_700</item>
        <item name="colorOnPrimary">@color/ie_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/ie_teal_200</item>
